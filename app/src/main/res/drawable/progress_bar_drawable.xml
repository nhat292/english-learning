<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                android:angle="270"
                android:centerColor="#9964B5F6"
                android:centerY="0.75"
                android:endColor="#ff0D47A1"
                android:startColor="#771E88E5" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:angle="270"
                    android:centerColor="#80039BE5"
                    android:centerY="0.75"
                    android:endColor="#a029B6F6"
                    android:startColor="#8001579B" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:angle="270"
                    android:endColor="#D50000"
                    android:startColor="#FF1744" />
            </shape>
        </clip>
    </item>
</layer-list>